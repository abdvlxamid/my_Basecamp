<p style="color: green"><%= notice %></p>

<h1>Messages</h1>

<div id="messages">
  <% @messages.each do |message| %>
    <%= render message %>
    <p>
      <%= link_to "Show this message", [@project, @inbox, message] %>
    </p>
  <% end %>
</div>

<%= link_to "New message", new_project_inbox_message_path %>
