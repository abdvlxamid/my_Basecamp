<main class="content-wrap">
		<header class="content-head">
			<h1>Topics</h1>
				
			<div class="action">
				<%# <button> %>
        <%= link_to 'New Topic', new_project_inbox_path(@project), class: 'button-18' %>
		<%= link_to 'Back', project_path(@project), class: 'button-18' %>
				<%# </button> %>
			</div>
		</header> <br />
		
		<div class="content">

		
			<section class="person-boxes">
      
      <% @inboxes.each do |inbox| %>
        
      
				<div class="person-box">
        
					
					<div class="box-bio">
						<h2 class="bio-name"><%= inbox.name %></h2>
						<p class="bio-position"></p>
					</div>
					
					<div class="box-actions">
						<button>
            <%= link_to [@project, inbox] do %>
